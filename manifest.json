{
	"name": "Fontastic",
	"author": "ToastyYogurtTime",
	"manifest_version": 2,
	"description": "Replace fonts from CDNs with local copies.",
	"version": "0.4",

	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"webNavigation"
	],

	"web_accessible_resources": [
		"fonts/*/*.woff2",
		"icons/*/*.woff2"
	],
	

	"background": {
		"scripts": ["fontastic.js"]
	}
}
